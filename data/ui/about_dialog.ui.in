<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAboutDialog" id="about_dialog">
    <property name="program_name">@prettyname@</property>
    <property name="version">@VERSION@</property>
    <property name="authors">@CONTRIBUTORS@</property>
    <property name="copyright">@authorfullname@, et al.</property>
    <property name="comments" translatable="yes">This application is not affiliated with or endorsed by Discord, Inc.</property>
    <property name="logo_icon_name">@APPID@</property>
    <property name="license_type">gpl-3-0</property>
  </object>
</interface>
